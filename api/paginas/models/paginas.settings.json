{
  "kind": "collectionType",
  "collectionName": "paginas",
  "info": {
    "name": "Paginas"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "nombre": {
      "type": "string",
      "required": true
    },
    "contenido": {
      "type": "text",
      "required": true
    },
    "imagen": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    }
  }
}
